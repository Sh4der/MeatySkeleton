# Copied and modified from: https://github.com/rofrol/react-starter-kit/blob/master/.editorconfig
root = true

# Unix style newlines with a newline ending every file
[*]
end_of_line = lf

# Matches multiple files with brace expansion notation
# Set default charset
charset = utf-8
indent_style = tab
indent_size = 4
trim_trailing_whitespace = true
